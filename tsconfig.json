{
    "compilerOptions": {
        "outDir": "./dist",
        "allowJs": true,
        "target": "es5",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "inlineSources": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "lib": [
            "ES5",
            "ES2015.Promise",
            "dom"
        ],
    },
    "include": [
        "./lib/**/*"
    ]
}
